    <string name="no_internet_msg">Impossible de se connecter au serveur,\nveuillez verifier votre connexion internet \n</string>
    <string name="utils_font_bold">bank_font/GoogleSans-Bold.ttf</string>
    <string name="utils_font_medium">bank_font/GoogleSans-Medium.ttf</string>
    <string name="utils_font_regular">bank_font/GoogleSans-Regular.ttf</string>
    <string name="utils_font_semi_bold">bank_font/GoogleSans-Medium.ttf</string>
    <declare-styleable name="Utils_CircleImageView">
        <attr format="string" name="name"/>
        <attr format="dimension" name="civ_border_width"/>
        <attr format="color" name="civ_border_color"/>
        <attr format="boolean" name="civ_border_overlay"/>
        <attr format="color" name="civ_circle_background_color"/>
        <attr format="color" name="civ_fill_color"/>
    </declare-styleable>
    <declare-styleable name="Utils_DotsIndicator">
        <attr format="dimension" name="dot_width"/>
        <attr format="dimension" name="dot_height"/>
        <attr format="dimension" name="dot_margin"/>

        <attr format="reference" name="dot_drawable"/>
        <attr format="reference" name="dot_drawable_unselected"/>
        <attr format="color" name="dot_tint"/>

        <attr format="reference" name="dots_animator"/>
        <attr format="reference" name="dots_animator_reverse"/>

        <attr format="enum" name="theme5_dots_orientation">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>

        <attr name="Utils_dots_gravity">
            <flag name="top" value="0x30"/>
            <flag name="bottom" value="0x50"/>
            <flag name="left" value="0x03"/>
            <flag name="right" value="0x05"/>
            <flag name="center_vertical" value="0x10"/>
            <flag name="fill_vertical" value="0x70"/>
            <flag name="center_horizontal" value="0x01"/>
            <flag name="fill_horizontal" value="0x07"/>
            <flag name="center" value="0x11"/>
            <flag name="fill" value="0x77"/>
            <flag name="clip_vertical" value="0x80"/>
            <flag name="clip_horizontal" value="0x08"/>
            <flag name="start" value="0x00800003"/>
            <flag name="end" value="0x00800005"/>
        </attr>
    </declare-styleable>
</resources>